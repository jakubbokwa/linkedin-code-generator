generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id          String   @id @default(cuid())
  email       String
  basePrompt  String
  chosenPost  String
  photoData   String?   // Data URL (base64) lub URL do pliku
  createdAt   DateTime  @default(now())

  @@index([createdAt])
}